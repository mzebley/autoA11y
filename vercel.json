{
  "cleanUrls": true,
  "trailingSlash": false,
  "framework": "astro",
  "buildCommand": "npm run build && npm run docs:build",
  "installCommand": "npm install --include=dev && npm --prefix docs install",
  "outputDirectory": "docs/dist",
  "headers": [
    {
      "source": "/dist/(.*)",
      "headers": [
        { "key": "Cache-Control", "value": "public, max-age=31536000, immutable" }
      ]
    }
  ]
}
