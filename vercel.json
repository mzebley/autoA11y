{
  "cleanUrls": true,
  "trailingSlash": false,
  "framework": null,
  "buildCommand": "npm run build",
  "installCommand": "npm install --include=dev",
  "outputDirectory": ".",
  "headers": [
    {
      "source": "/dist/(.*)",
      "headers": [
        { "key": "Cache-Control", "value": "public, max-age=31536000, immutable" }
      ]
    }
  ]
}
